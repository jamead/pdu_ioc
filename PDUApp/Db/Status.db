record(aSub, "$(P)DecodePDUStatus-Sub"){
    field(SNAM, "StatusSub")

    field(FTA,  "FLOAT")
    field(NOA,  "20")
    field(INPA, "$(P)PDU:Status-Wfm")
    field(FLNK, "$(P)DecodeChanStatus-Sub")

    field(FTVA,  "LONG")
    field(NOVA,  "1")
    field(OUTA, "$(P)Frame:Start-I PP")
    field(FTVB,  "FLOAT")
    field(NOVB,  "1")
    field(OUTB, "$(P)Current:PhaseA-I PP")
    field(FTVC,  "FLOAT")
    field(NOVC,  "1")
    field(OUTC, "$(P)Current:PhaseB-I PP")
    field(FTVD,  "FLOAT")
    field(NOVD,  "1")
    field(OUTD, "$(P)Current:PhaseC-I PP")
    field(FTVE,  "LONG")
    field(NOVE,  "1")
    field(OUTE, "$(P)CH1:Status-I PP")
    field(FTVF,  "LONG")
    field(NOVF,  "1")
    field(OUTF, "$(P)CH2:Status-I PP")
    field(FTVG,  "LONG")
    field(NOVG,  "1")
    field(OUTG, "$(P)CH3:Status-I PP")
    field(FTVH,  "LONG")
    field(NOVH,  "1")
    field(OUTH, "$(P)CH4:Status-I PP")
    field(FTVI,  "FLOAT")
    field(NOVI,  "1")
    field(OUTI, "$(P)Temp:Front-I PP")
    field(FTVJ,  "FLOAT")
    field(NOVJ,  "1")
    field(OUTJ, "$(P)Temp:Rear-I PP")
    field(FTVK,  "LONG")
    field(NOVK,  "1")
    field(OUTK, "$(P)Serial:Number-I PP")
    field(FTVL,  "LONG")
    field(NOVL,  "1")
    field(OUTL, "$(P)Model:Number-I PP")
    field(FTVM,  "LONG")
    field(NOVM,  "1")
    field(OUTM, "$(P)Firmware:Version-I PP")
    field(FTVN,  "LONG")
    field(NOVN,  "1")
    field(OUTN, "$(P)Watchdog:Flag-I PP")
    field(FTVO,  "LONG")
    field(NOVO,  "1")
    field(OUTO, "$(P)Frame:End-I PP")
}

