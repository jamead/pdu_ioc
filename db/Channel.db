record(stringout,"$(P)IP:Settings-SP"){
  field(DTYP,"Soft Channel")
  field(DESC,"IP Address:Port #")
}

record(mbbi, "$(P)UDP:Status-I") {
    field(DESC, "Set CH1 Fault")
    field(ZRST,"OK")
    field(ONST,"Socket\nFailed")
    field(TWST,"Invalid\nAddress")
    field(THST,"Send\nFailed")
    field(FRST,"Recv\nFailed")
    field(ZRVL,"0")
    field(ONVL,"1")
    field(TWVL,"2")
    field(THVL,"3")
    field(FRVL,"4")
}

